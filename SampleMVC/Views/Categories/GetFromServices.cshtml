@model IEnumerable<SampleMVC.Models.Category>

@{
    ViewData["Title"] = "GetFromServices";
}

<h1>GetFromServices</h1>

<p>
    @Html.ActionLink("Create New", "InsertFromServices", "Categories")

</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.categoryID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.categoryName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.categoryID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.categoryName)
            </td>
            <td>
                @Html.ActionLink("Edit", "EditFromServices", "Categories", new { id = item.categoryID })
                    @Html.ActionLink("Delete", "DeleteFromServices", "Categories", new { id = item.categoryID })
            </td>
        </tr>
}
    </tbody>
</table>
